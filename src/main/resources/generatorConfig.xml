<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="Mysql">
  	<commentGenerator>  
        <!-- 是否去除自动生成的注释 true：是 ： false:否 -->  
        <property name="suppressAllComments" value="true"/>  
    </commentGenerator> 
    <jdbcConnection connectionURL="jdbc:mysql://127.0.0.1:3306/rebot" driverClass="com.mysql.jdbc.Driver" password="123456" userId="root" />
    <javaModelGenerator targetPackage="com.seadun.rebot.entity" targetProject="rebot/src/main/java" />
    <sqlMapGenerator targetPackage="com.seadun.rebot.mapper" targetProject="rebot/src/main/java" />
    <javaClientGenerator targetPackage="com.seadun.rebot.mapper" targetProject="rebot/src/main/java" type="XMLMAPPER" />
    <table schema="rebot" tableName="user" domainObjectName="User"  enableCountByExample="false" enableUpdateByExample="false"  
            enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
  </context>
</generatorConfiguration>